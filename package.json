{
  "name": "shifting-express-to-functions",
  "version": "0.0.0",
  "scripts": {
    "local:build": "tsc -p server",
    "local:watch": "tsc -w -p server",
    "node:lint": "npm run node:build-ts && eslint \"server/**/*.ts\" --quiet --fix",
    "node:debug": "WWW=./dist/vacationwishlist nodemon -r dotenv/config --inspect=9229 server/dist/index.js",
    "node:start": "WWW=./dist/vacationwishlist node -r dotenv/config server/dist/index.js"
  },
  "private": true,
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "tslint": "~6.1.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.8",
    "@types/node": "^13.13.20",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "bulma": "^0.8.2",
    "codelyzer": "^5.2.2",
    "concurrently": "^5.3.0",
    "core-js": "^3.6.5",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "nodemon": "^2.0.4",
    "ts-node": "^8.10.2",
    "tslib": "^2.0.0", 
    "typescript": "~4.0.2"
  }
}
